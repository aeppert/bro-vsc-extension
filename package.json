{
	"name": "bro",
	"displayName": "bro/zeek",
	"description": "Bro/Zeek Script Language Syntax Highlighting",
	"version": "0.6.0",
	"publisher": "aeppert",
	"engines": {
		"vscode": "^1.12.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aeppert/bro-vsc-extension.git"
	},
	"categories": [
		"Languages",
		"Formatters",
		"Snippets"
	],
	"keywords": [
        "bro",
        "bro-script",
        "zeek",
        "zeek-script"
	],
	"contributes": {
		"languages": [
			{
				"id": "zeek",
				"aliases": [
					"Zeek",
					"Bro",
                    "bro",
                    "zeek"
				],
				"extensions": [
					".bro",
					".zeek"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "zeek",
				"scopeName": "source.zeek",
				"path": "./syntaxes/bro.json"
			}
		]
	}
}
