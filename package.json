{
	"name": "bro",
	"displayName": "bro/zeek",
	"description": "Bro/Zeek Script Language Syntax Highlighting",
	"version": "0.7.0",
	"publisher": "aeppert",
	"engines": {
		"vscode": "^1.12.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/aeppert/bro-vsc-extension.git"
	},
	"categories": [
		"Programming Languages",
		"Formatters",
		"Snippets"
	],
	"keywords": [
        "bro",
        "bro-script",
        "zeek",
        "zeek-script"
	],
	"contributes": {
		"languages": [
			{
				"id": "zeek",
				"aliases": [
					"Zeek",
					"Bro",
                    "bro",
                    "zeek"
				],
				"extensions": [
					".bro",
					".zeek"
				],
				"filenames": [
					"events.bif",
					"consts.bif",
					"types.bif"
				],
				"configuration": "./language-configuration.json"
			},
			{
				"id": "sig",
				"aliases": [
					"Signature",
					"signature",
					"Sig",
					"sig"
				],
				"extensions": [
					".sig"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "zeek",
				"scopeName": "source.zeek",
				"path": "./syntaxes/bro.json"
			},
			{
				"language": "sig",
				"scopeName": "source.sig",
				"path": "./syntaxes/sig.json"
			}
		],
		"snippets": [
			{
				"language": "zeek",
				"scopeName": "source.zeek",
				"path": "./snippets/bro.json"
			}
		]
	}
}
